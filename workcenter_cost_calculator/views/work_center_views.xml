<odoo noupdate="1">
    <!-- Form View -->
    <record id="view_work_center_form" model="ir.ui.view">
        <field name="name">work.center.form</field>
        <field name="model">work.center</field>
        <field name="arch" type="xml">
            <form string="Work Center">
                <group>
                    <field name="name"/>
                    <field name="hourly_cost"/>
                </group>
            </form>
        </field>
    </record>

    <!-- Tree View -->
    <record id="view_work_center_tree" model="ir.ui.view">
        <field name="name">work.center.tree</field>
        <field name="model">work.center</field>
        <field name="arch" type="xml">
            <tree string="Work Centers">
                <field name="name"/>
                <field name="hourly_cost"/>
            </tree>
        </field>
    </record>

